@{
    ViewBag.Title = "锦标赛";
}
<link href="@Url.Content("~/Content/asset/css/common.css?1.1.0")" rel="stylesheet" type="text/css" />
<link href="~/Content/asset/css/judgetourname.css?1.1.0" rel="stylesheet" />
<link href="@Url.Content("~/Content/asset/css/judgetour.css?2.1.0")" rel="stylesheet" type="text/css" />
@Html.Partial("_judgetop")
<div class="container judge-compe">
    <div class="pi-top competitle">
        <img src="~/Content/asset/img/tour_03.png" />
