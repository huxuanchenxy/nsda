<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta HTTP-EQUIV="Pragma" CONTENT="no-cache">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <!-- uc强制竖屏 -->
    <meta name="screen-orientation" content="portrait">
    <!-- QQ强制竖屏 -->
    <meta name="x5-orientation" content="portrait">
    <!-- UC强制全屏 -->
    <meta name="full-screen" content="yes">
    <!-- QQ强制全屏 -->
    <meta name="x5-fullscreen" content="true">
    <!-- UC应用模式 -->
    <meta name="browsermode" content="application">
    <!-- QQ应用模式 -->
    <meta name="x5-page-mode" content="app">
    <!-- windows phone 点击无高光 -->
    <meta name="msapplication-tap-highlight" content="no">

    <title>@ViewBag.Title - NSDA</title>
    @Styles.Render("~/Content/asset/css/bootstrap.min.css")
    @Styles.Render("~/Content/asset/css/basic.css")
    @Styles.Render("~/Content/asset/css/iconfont.css?6.1.0")
    @Styles.Render("~/Content/asset/css/index.css?1.1.1")
    @*@Scripts.Render("~/Scripts/jquery-3.2.1.min.js")*@
    @Scripts.Render("~/Content/asset/js/jquery.min.js")
    <!--[if IE]>
    @Scripts.Render("~/Content/asset/js/jquery.min.js")
    <![endif]-->
    @Scripts.Render("~/Content/asset/js/parallax.min.js")
    @Scripts.Render("~/Content/asset/js/bootstrap.min.js")
    @Scripts.Render("~/Content/asset/js/laydate/laydate.js")

</head>
<body>
    <nav class="nav navbar-inverse nav-top navbar-fixed-top">
        <div class="container">
            <div class="collapse navbar-collapse">
                <div class="container search navbar-right">
                    <a href="/Player/playerregform" class="navbar-link link"> &nbsp;|&nbsp; 注册 </a><a href="/Main/registered" class="navbar-link link">登录</a>
                    <span class="iconfont icon-yonghu" aria-hidden="true"></span>
                    <a href="" id="check"><span class="iconfont icon-sousuo" aria-hidden="true"></span></a>
                    <input type="text" class="form-control" name="" placeholder="">


                </div>

            </div>
        </div>
    </nav>
    <header>
        <nav class="navbar navbar-default header container" role="navigation">
            <div class="navbar-header">

                <button type="button" class="navbar-toggle" data-toggle="collapse"
                        data-target="#example-navbar-collapse">

                    <span class="sr-only">切换导航</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="javascript:;"></a>

            </div>
            <div class="collapse navbar-collapse" id="example-navbar-collapse">
                <ul class="nav navbar-nav drop-list" id="lead">
                    <li class="">@Html.ActionLink("首页", "index", "Main")</li>
                    <li class="dropdown">
                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                            关于我们
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("关于NSDA", "About", "Main")</li>
                            <li><a href="javascript:;">NSDA中国</a></li>
                            <li>@Html.ActionLink("合作伙伴", "Aboutp", "Main")</li>
                            <li>@Html.ActionLink("联系我们", "Aboutus", "Main")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                            会员系统
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="javascript:;">会员系统</a></li>
                            <li>@Html.ActionLink("裁判招募", "judgelogin", "Judge")</li>
                            <li>@Html.ActionLink("申请赛事管理员", "eventmanagelogin", "Eventmanage")</li>
                            <li><a href="#">排名榜单</a></li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            赛事
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="javascript:;">赛历</a></li>
                            <li><a href="javascript:;">国内赛事</a></li>
                            <li><a href="javascript:;">国际赛事</a></li>
                        </ul>
                    </li>
                    <li class=""><a href="#">课程专栏</a></li>

                    <li class="dropdown">
                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                            更多
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="javascript:;">辩题</a></li>
                            <li><a href="javascript:;">教材购买</a></li>
                        </ul>
                    </li>
                </ul>


                <div class="sear hidden-sm hidden-md hidden-lg">
                    <input type="text" class="form-control" name="" placeholder="">
                    <a href="" class="check2"><span class="iconfont icon-sousuo" aria-hidden="true"></span></a>
                    <span class="iconfont icon-yonghu" aria-hidden="true"></span>
                    <a href="/Home/Regform" class="navbar-link link">注册 | </a><a href="/Home/Registered" class="navbar-link link">&nbsp;登录 </a>
                </div>
            </div>
        </nav>
    </header>

    <div class="body-content">
        @RenderBody()

    </div>
    <!--联系我们-->

    @*<div class="blank">

        </div>*@
    <footer>
        <p>Copyright © 2005 - 2017 National Forensic League of China All Reserved</p>
    </footer>

    @RenderSection("scripts", required: false)
</body>
</html>

