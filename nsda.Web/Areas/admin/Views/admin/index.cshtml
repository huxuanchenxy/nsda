    <link rel="stylesheet" href="~/Areas/admin/asset/css/index.css"/>
    <link rel="stylesheet" href="~/Areas/admin/asset/css/base.css" />
</head>
<body class="hold-transition skin-blue sidebar-mini" style="overflow:hidden;">
    <div class="wrapper">
        <header class="main-header">
            <a href="/home/index" class="logo">
                <span class="logo-mini" style="overflow-x:hidden">nsda</span>
                <img src="~/Areas/admin/asset/img/logo.jpg" style="margin-left:-50px;" />
            </a>
            <nav class="navbar navbar-static-top">
                <a class="sidebar-toggle">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="/asset/img/avatar.png" class="user-image" alt="User Image">
                                <span class="hidden-xs">nsda管理员</span>
                                <b class="caret"></b>
                            </a>
                            <ul class="dropdown-menu pull-right">
                                <li><a class="menuItem" data-menuid="/admin/home/info" href="/admin/home/info"><i class="fa fa-user"></i>个人中心</a></li>
                                <li><a href="javascript:void(0)" id="exit"><i class="ace-icon fa fa-power-off"></i>安全退出</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>

        <div class="main-sidebar">
            <div class="sidebar">
                <ul class="sidebar-menu" id="sidebar-menu"></ul>
            </div>
        </div>

        <div id="content-wrapper" class="content-wrapper">
            <div class="content-tabs">
                <button class="roll-nav roll-left tabLeft">
                    <i class="fa fa-backward"></i>
                </button>
                <nav class="page-tabs menuTabs">
                    <div class="page-tabs-content" style="margin-left: 0px;">
                        <a href="javascript:;" class="menuTab active" data-menuid="home1">控制台</a>
                    </div>
                </nav>
                <button class="roll-nav roll-right tabRight">
                    <i class="fa fa-forward" style="margin-left: 3px;"></i>
                </button>
                <div class="btn-group roll-nav roll-right">
                    <button class="dropdown tabClose" data-toggle="dropdown">
                        关闭操作<i class="fa fa-caret-down" style="padding-left: 3px;"></i>
                    </button>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li><a class="tabReload" href="javascript:void();">刷新当前</a></li>
                        <li><a class="tabCloseCurrent" href="javascript:void();">关闭当前</a></li>
                        <li><a class="tabCloseAll" href="javascript:void();">关闭全部选项卡</a></li>
                        <li><a class="tabCloseOther" href="javascript:void();">关闭其他选项卡</a></li>
                    </ul>
                </div>
                <button class="roll-nav roll-right fullscreen"><i class="fa fa-arrows-alt"></i></button>
            </div>
            <div class="content-iframe" style="overflow: hidden;">
                <div class="mainContent" id="content-main" style="margin:4px; padding: 0;">
                    <iframe class="fg_iframe" width="100%" height="100%" src="/admin/home/home" id="iframe0" name="iframe0" frameborder="0" data-menuid="home1"></iframe>
